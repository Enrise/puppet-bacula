# This file is managed by Puppet. DO NOT EDIT.

Pool {
  Name = "<%= @name %>"
  PoolType = <%= @type %>
  MaximumVolumeJobs = <%= @maximum_volume_jobs %>
<% if @maximum_volume_bytes != '' -%>
  MaximumVolumeBytes = <%= @maximum_volume_bytes %>
<% end -%>
  UseVolumeOnce = <%= @use_volume_once %>
  Recycle = <%= @recycle %>
  ActionOnPurge = <%= @action_on_purge %>
  AutoPrune = <%= @auto_prune %>
  VolumeRetention = <%= @volume_retention %>
  LabelFormat = "<%= @label_format %>"
<% if @storage != '' -%>
  Storage = "<%= @storage %>"
<% end -%>
}
