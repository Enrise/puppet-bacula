# This file is managed by Puppet. DO NOT EDIT.

Client {
  Name = "<%= @name %>"
  Address = <%= @address %>
<% if @port != '' -%>
  FDPort = <%= @port %>
<% end -%>
<% if @real_password != '' -%>
  Password = "<%= @real_password %>"
<% end -%>
  Catalog = "<%= @catalog %>"
<% if @file_retention != '' -%>
  FileRetention = <%= @file_retention %>
<% end -%>
<% if @job_retention != '' -%>
  JobRetention = <%= @job_retention %>
<% end -%>
<% if @auto_prune != '' -%>
  AutoPrune = <%= @auto_prune %>
<% end -%>
<% if @max_concurrent != '' -%>
  MaximumConcurrentJobs = <%= @max_concurrent %>
<% end -%>
<% if scope.lookupvar('bacula::heartbeat_interval') != '' -%>
  HeartbeatInterval = <%= scope.lookupvar('bacula::heartbeat_interval') %>
<% end -%>
}
