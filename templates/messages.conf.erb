# This file is managed by Puppet. DO NOT EDIT.
  
Messages {
  Name = <%= @name %>
  mailcommand = "<%= @mail_command %> -h <%= @mail_host %> -f \"<%= @mail_from= %>" -s \"Bacula: %t %e of %c %l\" %r"
  operatorcommand = "/usr/bin/bsmtp -h localhost -f \"Bacula CMP <noreply@mail_from=>\" -s \"Bacula: Intervention needed for %j\" %r"
  mail = <%= @mail_to %> = all, !skipped            
  operator = system-notifications@cm-pueyrredon.com.ar = mount
  console = all, !skipped, !saved
  catalog = all, !skipped, !saved
  mailonerror = bacula-backup@example.com = all
  append = "/var/log/bacula/log" = all, !skipped
}
